{
  "type": "module",
  "scripts": {
    "build": "astro check && tsc && astro build",
    "dev": "concurrently --raw \"npm:dev:*\"",
    "dev:astro": "astro dev --host",
    "dev:css": "chokidar \"src/styles/**/*.css\" --command \"touch tailwind.config.cjs\" --debounce 0 --silent",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "concurrently --raw \"npm:lint:*\"",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:prettier": "prettier . --check",
    "preview": "astro preview",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@alpinejs/collapse": "3.13.7",
    "@alpinejs/focus": "3.13.5",
    "@alpinejs/intersect": "3.13.7",
    "@alpinejs/persist": "3.13.10",
    "@alpinejs/ui": "3.13.5-beta.0",
    "@astrojs/check": "0.5.4",
    "@astrojs/mdx": "2.3.1",
    "@astrojs/sitemap": "3.1.4",
    "@astrojs/tailwind": "5.1.0",
    "@studio-freight/lenis": "1.0.42",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/alpinejs": "3.13.6",
    "@types/three": "0.162.0",
    "@vercel/edge": "1.1.1",
    "alpinejs": "3.13.5",
    "astro": "4.5.12",
    "gsap": "3.12.5",
    "outvariant": "1.4.2",
    "postprocessing": "6.35.2",
    "rollup-plugin-visualizer": "5.12.0",
    "split-type": "0.3.4",
    "tailwindcss-animate": "1.0.7",
    "three": "0.162.0",
    "tiny-invariant": "1.3.3",
    "vite-plugin-glsl": "1.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.1.1",
    "chokidar-cli": "3.0.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-astro": "0.31.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-tailwindcss": "3.14.3",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-css-order": "2.0.1",
    "prettier-plugin-tailwindcss": "0.5.11",
    "vitest": "1.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "private": true
}
